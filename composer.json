{
  "name": "isponsor/object-foundation",
  "description": "Atomic ontology traits for PHP entities (Doctrine/Symfony ready).",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Oleksandr Tishchenko (with GPT)",
      "email": "example@example.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "doctrine/orm": "^2.16 || ^3.0",
    "symfony/uid": "^6.4 || ^7.0",
    "symfony/serializer": "^6.4 || ^7.0",
    "symfony/validator": "^6.4 || ^7.0",
    "symfony/console": "^6.4 || ^7.0",
    "symfony/event-dispatcher": "^6.4 || ^7.0",
    "ext-openssl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6 || ^10.5"
  },
  "autoload": {
    "psr-4": {
      "ObjectFoundation\\": "src/"
    },
    "files": [
      "src/SDK/autoload.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ObjectFoundation\\Tests\\": "tests/",
      "Examples\\SymfonyDemo\\": "tests/Examples/SymfonyDemo/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "test": "phpunit"
  },
  "bin": [
    "bin/foundation"
  ],
  "version": "2.3.0-beta",
  "extra": {
    "object-foundation-next-horizon": "visionary roadmap 1.5\u20133.0"
  },
  "suggest": {
    "object-foundation-bridge-symfony": "Bundle integration for Symfony",
    "object-foundation-bridge-laravel": "Service Provider integration for Laravel"
  }
}