{
  "name": "isponsor/object-foundation",
  "description": "Atomic ontology traits for PHP entities (Doctrine/Symfony ready).",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Oleksandr Tishchenko",
      "email": "taa0662621456@gmail.com",
      "homepage": "https://smartresponsor.com",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": ">=8.3",
    "doctrine/orm": "^2.16 || ^3.0",
    "monolog/monolog": "^3.6",
    "ext-openssl": "*",
    "predis/predis": "^3.2",
    "ramsey/uuid": "^4.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6 || ^10.5",
    "symfony/var-dumper": "^7.0",
    "symfony/console": "^6.4 || ^7.0",
    "symfony/yaml": "^6.4 || ^7.0",
    "phpstan/phpstan": "^1.11",
    "rector/rector": "^1.2",
    "friendsofphp/php-cs-fixer": "^3.20"
  },
  "autoload": {
    "psr-4": {
      "ObjectFoundation\\": "src/"
    },
    "files": [
      "src/SDK/autoload.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ObjectFoundation\\Tests\\": "tests/",
      "Examples\\SymfonyDemo\\": "tests/Examples/SymfonyDemo/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "test:atomicity": [
      "@php scripts/atomicity-check.php"
    ]
  },
    "test": "phpunit --testdox",
    "lint": [
      "phpstan analyse src --level=8 --memory-limit=1G",
      "rector process src --dry-run",
      "php-cs-fixer fix --dry-run --diff"
    ],
    "bin": [
      "bin/foundation"
  ],
  "version": "2.7.1-beta",
  "extra": {
    "object-foundation-next-horizon": "visionary roadmap 1.5\u20133.0"
  },
  "suggest": {
    "object-foundation-bridge-symfony": "Bundle integration for Symfony",
    "object-foundation-bridge-laravel": "Service Provider integration for Laravel"
  }
}
