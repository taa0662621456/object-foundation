#!/usr/bin/env php
<?php
require __DIR__ . '/../vendor/autoload.php';

use Symfony\Component\Console\Application;
use ObjectFoundation\Command\FoundationEntityInfoCommand;

$application = new Application('Object Foundation CLI', '1.x');
$application->add(new FoundationEntityInfoCommand());
$application->run();

use ObjectFoundation\Command\FoundationConfigEncryptCommand;
use ObjectFoundation\Command\FoundationConfigRotateKeyCommand;

$application->add(new FoundationConfigEncryptCommand());
$application->add(new FoundationConfigRotateKeyCommand());
